{{#each this}}
  <li class='country_item'>
    <div class='country_flag'>
      <img src='{{flag}}' alt='flag ' width='320' />
    </div>
    <h2>{{name}}</h2>
    <p><b>capital: {{capital}}</b></p>
    <p><b>currency: {{currency}}</b></p>
    <p><b>language: {{languages}}</b> </p>
    <p><b>population: {{population}}</b></p>
  </li>
{{/each}}